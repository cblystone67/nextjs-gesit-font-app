  "success_stories": [
    {
      "id": 1,
      "name": "Marcus J.",
      "program": "Seattle Central College - Automotive Technology",
      "story": "After 8 years of incarceration, I thought my chances of getting a good education were over. The reentry program at Seattle Central not only helped me get my GED, but I went on to complete their automotive technology program. Now I'm working as a certified mechanic and have been promoted to shop supervisor.",
      "outcome": "Employed as automotive shop supervisor",
      "timeframe": "Completed program in 18 months"
    },
    {
      "id": 2,
      "name": "Jennifer S.",
      "program": "University of Washington - Social Work",
      "story": "The UW Reentry Program changed my life. I went from having a criminal record and no college education to earning my Bachelor's in Social Work. Now I work with other women who are reentering society, helping them navigate the same challenges I faced.",
      "outcome": "Bachelor's degree, working as social worker",
      "timeframe": "3 years from start to graduation"
    },
    {
      "id": 3,
      "name": "Robert M.",
      "program": "Renton Technical College - Construction",
      "story": "I learned construction skills in prison, but getting certified and finding work was the challenge. RTC's reentry support helped me get my certifications and connected me with employers who were willing to give me a chance. I now run my own small construction business.",
      "outcome": "Business owner, certified contractor",
      "timeframe": "1 year program, business started 2 years later"
    }
  ]
